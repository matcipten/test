                            <label class="slds-form-element__label">{!v.translatorValue.Signature_Label__c}</label>
                            <div class="{! 'canvas_spad' + ' ' +  (v.locale == 'CN' ? '' : 'margin-cn' )}" >
                                <canvas id="canvas" aura:id="canvas"></canvas>
                            </div>
                            <aura:if isTrue="{!v.locale == 'CN'}">
                                <label class="slds-form-element__label margin-cn">{!v.translatorValue.Signature_Asterisco_Label__c}</label>
                            </aura:if>
                            <div class="slds-form-element noPrint">
                                <lightning:button class="slds-button slds-button_neutral noPrint" label="{!v.translatorValue.Button_Signature_Clear_Label__c}" onclick="{!c.sPadClear}"/>
                            </div>
                            <lightning:input aura:id="quoteField" name="name" label="Name" value="{!v.sPad64}" required="true" class="slds-hidden" />
                        </div>
                    </div>
                </div>
                
                <div class="slds-grid slds-wrap row slds-text-align_center noPrint">
