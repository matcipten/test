                            <lightning:button class="slds-button slds-button_neutral" label="{!v.translatorValue.Button_Form_Registration_Label__c}" onclick="{!c.validateAndRegistration}" disabled="{!v.buttonDisable}" />
                        </div>
                    </div>
                </div>
                
                <div class="page-break"></div>
                <div class='privacy'>
                    <br/>
                    <br/>
                    <div class="header-logo-privacy">
                        <img src="/resource/logoBC"/>
                    </div>
                    <!--###### MODAL BOX BODY Part Start From Here ######-->
                    <div>
                        <div class="header-text-policy">{!v.translatorValue.Informativa_Header_Label__c}</div>
                        <aura:if isTrue="{!v.user.Department__c}">
                        	<ui:outputRichText class="text-rich-privacy" aura:id="outputRT" value="{!v.translatorValue.Informativa_Body_Rich_Label_Department__c}" />                       
                            <aura:set attribute="else">
                        	<ui:outputRichText class="text-rich-privacy" aura:id="outputRT" value="{!v.translatorValue.Informativa_Body_Rich_Label__c}" />                       
                            </aura:set>
                        </aura:if>  
                    </div>
                    
                </div>
                
                <!--Use aura:if tag to display Model Box, on the bese of conditions. [isOpen boolean attribute] -->   
                <aura:if isTrue="{!v.isOpen}">
                    <!--###### MODAL BOX Start From Here ######--> 
                    <div aura:id="modal-privacy" role="dialog" tabindex="-1" aria-labelledby="header99" class="slds-modal slds-fade-in-open print">
                        <div class="slds-modal__container" id="modal-container" aura:id="modal-container" >
                            <!-- ###### MODAL BOX HEADER Part Start From Here ######-->
                            <div class="slds-modal__header">
                                <button class="slds-button slds-modal__close slds-button--icon-inverse noPrint" title="Close" onclick="{!c.closeModel}">
                                    X
                                    <span class="slds-assistive-text">Close</span>
                                </button>
                                <div class="header-logo-privacy">
                                    <img src="/resource/logoBC"/>
                                </div>
                            </div>
                            <!--###### MODAL BOX BODY Part Start From Here ######-->
                            <div class="slds-modal__content slds-p-around--medium">
                                <div class="slds-text-body_regular header-text-policy">{!v.translatorValue.Informativa_Header_Label__c}</div>
                                <aura:if isTrue="{!v.user.Department__c}">
                                    <div class="uiOutputRichText" linkify="true" >
                                        <aura:unescapedHtml value="{!v.translatorValue.Informativa_Body_Rich_Label_Department__c}"/>
                                    </div>
                                    <aura:set attribute="else">
                                        <div class="uiOutputRichText" linkify="true" >
                                        <aura:unescapedHtml value="{!v.translatorValue.Informativa_Body_Rich_Label__c}"/>
                                    </div>                       
                                    </aura:set>
                                </aura:if>
                                <!--iframe src="https://www.repubblica.it/"
                                        width="100%"
                                        height="100%"
                                        style=""
                                        frameBorder=""
                                        sandbox=""
                                        scrolling="yes"/-->
                            </div>
                        </div>
                    </div>
                    <div aura:id="modal-backdrop" class="slds-backdrop slds-backdrop--open"></div>
                    <!--###### MODAL BOX Part END Here ######-->
                </aura:if>
            </div>
        </div>
    </aura:if>
    
    <aura:if isTrue="{!v.successOpen}">
        <div class="container component" id="container" aura:id="container">
            <div class="header thanks">
                <div class="header-logo">
                    <img src="/resource/logoBC2" class="image-header-thanks" />
                </div>
                <div class="header-text-thanks">
                    {!v.translatorValue.Subheader_Label__c}<br />
                    <!--{!v.language}<br /> -->
                    <!--{!v.locale} -->
                </div>
                <div class="text-thanks">
                    <p>Grazie per la tua registrazione {!v.newItem.FirstName} &nbsp;{!v.newItem.LastName}</p>
                </div>
            </div>
            
            <div class="slds-grid slds-wrap row slds-text-align_center noPrint">
                <div class="slds-col slds-size_12-of-12 slds-large-size_12-of-12">
                    <div class="slds-form-element button-registration">
                        <!-- lightning:button label="Return" onclick="{!c.returnToForm}"/ -->
                    </div>
                </div>
            </div>
        </div>
    </aura:if>
    
</aura:component>