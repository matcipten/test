<aura:component controller="RegistrationForm" access="global" 
                implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction,lightning:actionOverride,lightning:isUrlAddressable" >
							
   

    <ltng:require scripts="/resource/JsBarcode" />
    <!-- ltng:require scripts="/resource/jquery_3_3_1"
                  afterScriptsLoaded="{!c.doInit}" / -->    
    
    <!-- Load the navigation events in the force namespace. -->

    <aura:attribute name="recordId" type="String" />
    <!-- Akshay 05/06/2019 -->
	<aura:attribute name="newAcc" type="Account"  default="{'sobjectType':'Account'}" />
    <aura:attribute name="newItem" type="Account"  default="{'sobjectType':'Account'}" />
    <aura:attribute name="user" type="User"  default="{'sobjectType':'User'}" />
    <aura:attribute name="sPad64" type="String" default="" />
    <aura:attribute name="myDateTime" type="Date" />
    <aura:attribute name="language" type="String"/>
    <aura:attribute name="locale" type="String"/>
    <aura:attribute name="truncLanguage" type="String"/>
    <aura:attribute name="translator" type="Map" />
    <aura:attribute name="multiLanguage" type="Boolean" />
    <aura:attribute name="isOpen" type="boolean" default="false"/>
    <aura:attribute access="private" name="errors" type="String[]" default=""/>
    <!-- MC- Date fields -->
    <aura:attribute name="dayBirthdate" type="String"/>
    <aura:attribute name="monthBirthdate" type="String"/>
    <aura:attribute name="yearBirthdate" type="String"/>
    <aura:attribute name="days" type="Integer[]"/>
    <aura:attribute name="months" type="Integer[]"/>


    <aura:attribute name="sid" type="String"/>
    
    <aura:attribute name="formOpen" type="boolean" default="true"/>
    <aura:attribute name="successOpen" type="boolean" default="false"/>
    <aura:attribute name="isUpdate" type="boolean" default="false"/>
    <aura:attribute name="isDisabled" type="boolean" default="true"/>
    <aura:attribute name="isValid" type="boolean" default="false"/>
    
    <aura:attribute name="afterPrinting" type="boolean" default="false"/>
    
    
    <aura:attribute name="translatorValue" type="Map" />    
    <aura:attribute name="buttonDisable" type="Boolean" default="false" />
    
    <aura:attribute name="Privacy_1_old" type="boolean" default="false"/>
    <aura:attribute name="Privacy_2_old" type="boolean" default="false"/>
    <aura:attribute name="Privacy_3_old" type="boolean" default="false"/>
    
    <aura:attribute name="DCCVersion_old" type="String"/>
    <aura:attribute name="TranslationDCCVersion" type="String"/>
    <aura:attribute name="TranslationRichiediFirma" type="boolean"/>
    <aura:attribute name="DCC" type="boolean"/>

    <aura:attribute name="viewBarcode" type="boolean"/>
    
    
    <aura:attribute name="Id" type="String" />
    
    
   
    <aura:attribute name="isexpanded" type="boolean" default="false"/>
       

    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    
    <aura:handler name="change"
	              value="{!v.pageReference}"
	              action="{!c.refreshCmp}" />
     <lightning:navigation aura:id="navService"/>

    <!-- PALUMBO (START) -->
    <aura:attribute name="showContactAddress" type="Boolean" default="false"/>
    <aura:attribute name="showOtherAddress" type="Boolean" default="false"/>
    <!-- PALUMBO (END) -->

    <!-- VS - 20/04/2020 ISSUE 745 -->
    <aura:attribute name="customPicklistCountry" type="Object" />
    <aura:attribute name="customPicklistCountryBU" type="Object" />

    <aura:attribute name="customPicklistNationality" type="Object" />
    <aura:attribute name="customPicklistNationalityBU" type="Object" />

    <aura:attribute name="customPicklistProfession" type="Object" />
    <aura:attribute name="customPicklistProfessionBU" type="Object" />
    <aura:attribute name="ProfessionBU" type="String"/>

    <aura:attribute name="customPicklistPreferredLanguage" type="Object" />
    <aura:attribute name="customPicklistPreferredLanguageBU" type="Object" />
    <!-- VS - 20/04/2020 ISSUE 745 -->
    

    <aura:attribute name="customPicklistHobby" type="Object" />
    <aura:attribute name="customPicklistHobbyBU" type="Object" />

    
    
    <aura:attribute name="customPicklistCountryContact" type="Object" />
    <aura:attribute name="customPicklistCountryContactBU" type="Object" />

    <aura:attribute name="customPicklistCountryOther" type="Object" />
    <aura:attribute name="customPicklistCountryOtherBU" type="Object" />

    <aura:attribute name="customPicklistPassportCountry" type="Object" />
    <aura:attribute name="customPicklistPassportCountryBU" type="Object" />
    <aura:attribute name="hobbyBackup" type="String[]" default=""/>

    <aura:attribute name="salutationBackuo" type="String"/>

    <aura:attribute name="GBBool" type="boolean" default="false"/>
    <aura:attribute name="DCCBool" type="boolean" default="true"/>

    <aura:attribute name="optSalutation" type="Object" />
    <aura:attribute name="optSesso" type="Object" />
    <aura:attribute name="optLang" type="Object" />
    <!-- VS 11/05/2020 -->
    <!-- <aura:handler name="registrationFormEvent" event="c:registrationFormEvent" action="{!c.handleComponentEvent}"/> -->

    <aura:attribute name="hideBody" type="Boolean" default="true"/>

    <aura:if isTrue="{!v.hideBody}">
        <lightning:spinner size="medium" alternativeText="Loading"/>
    </aura:if>

    <aura:renderIf isTrue="{!!v.hideBody}">

        <!-- REGISTRATION WITH TAX FREE -->
        <!-- VS 11/05/2020 -->
        <!-- <c:createCustomerTaxFreeComponent translations="{!v.translatorValue}" show="{!!v.DCCBool}" country="{!v.customPicklistCountry}"/> -->

        <!-- STANDARD CUSTOMER FORM -->
        <aura:renderIf isTrue="{!v.DCCBool}">
            <ltng:require scripts="/resource/sPad"
            afterScriptsLoaded="{!c.doneRendering}" />

            <aura:if isTrue="{!v.formOpen}">
                
                <!-- <div class="spinnerDiv">
                    <lightning:spinner aura:id="mySpinner" class=""/>
                </div> -->
                
                <aura:if isTrue="{!v.isUpdate}">
                    <div class="noPrint text-update" style="color: #d9dbdd; padding:5px;">
                        <p><span class="noPrint" style="font-weight: bold;">Update: </span><span class="noPrint" style="font-style: italic;">{!v.newItem.FirstName}&nbsp;{!v.newItem.LastName}</span></p>
                    </div>
                </aura:if>
                <div class="container component" id="container" aura:id="container">
                    <div class="header">
                        <div class="header-logo">
                            <img src="/resource/logoBC2" class="image-header"/>
                        </div>
                        <div class="header-text">
                            {!v.translatorValue.Subheader_Label__c}<br />
                            <!--{!v.language}<br /> -->
                            <!--{!v.locale} -->
                        </div>
                    </div>
                    
                    <div class="slds-form slds-form_stacked">
                        <div aura:id="picklistShow" class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_2-of-12 slds-large-size_1-of-12 language-picker">
                                <div class="slds-form-element">
                                    <label class="slds-form-element__label" for="input-id-01">{!v.translatorValue.Language_Label__c}</label>
                                    <div class="slds-form-element__control">
                                        <ui:inputSelect aura:id="InputSelectDynamicLanguage" class="dynamic slds-select" change="{!c.onPicklistChange}" options="{!v.optLang}"/>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <aura:if isTrue="{! !empty(v.errors)}">
                            
                            <div aura:id="errors-container" class="slds-grid slds-wrap row padding slds-theme_alert-texture slds-theme_error noPrint" role="alert">
                                <div class="slds-col slds-size_12-of-12 slds-large-size_12-of-12">
                                    <div class="slds-form-element">
                                        
                                        <button class="slds-button slds-button_icon slds-float_right" onclick="{!c.closeError}">
                                            <lightning:buttonIcon iconName="utility:close" variant="bare" size="small"  alternativeText="expand" />
                                            
                                            <span class="slds-assistive-text">Provide description of action</span>
                                        </button>
                                    </div>
                                </div>
                                
                                
                                <aura:iteration items="{!v.errors}" var="r">
                                    <div class="slds-col slds-size_12-of-12 slds-large-size_12-of-12">
                                        <div class="slds-form-element">
                                            <li>{!r}</li>
                                        </div>
                                    </div>
                                </aura:iteration>
                                
                            </div>
                        </aura:if>
                        <div class="slds-grid slds-wrap row">
                            <div class="slds-col slds-size_2-of-12 slds-large-size_2-of-12">
                                <div class="slds-form-element">
                                    <label class="slds-form-element__label" for="input-id-01">{!v.translatorValue.Salutation_Label__c}</label>
                                    <div class="slds-form-element__control">
                                        <ui:inputSelect class="dynamic slds-select" value="{!v.newItem.Salutation}" aura:id="InputSelectDynamicTitolo" disabled="{!v.afterPrinting}" options="{!v.optSalutation}"/>
                                    </div>
                                </div>
                            </div>
                            <aura:if isTrue="{!(v.locale == 'JP'|| v.locale == 'CN' || v.locale == 'RU' || v.locale == 'HK' || v.locale == 'MO' || v.truncLanguage == 'ja'|| v.truncLanguage == 'zhs' || v.truncLanguage == 'zh' || v.truncLanguage == 'ru' )}">
                                <!-- JP/CN -->
                                <div class="slds-col slds-size_5-of-12 slds-large-size_5-of-12">
                                    <div class="slds-form-element">
                                        <label class="slds-form-element__label" for="input-id-01">{!v.translatorValue.Last_Name_Label__c}</label>
                                        <div class="slds-form-element__control">
                                            <ui:inputText aura:id="itemSurnameJC" class="slds-input" placeholder="{!v.translatorValue.Last_Name_Placeholder__c}" value="{!v.newItem.LastName}" onError="{!c.handleError}" onClearErrors="{!c.handleClearError}" disabled="{!v.afterPrinting}" />
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-col slds-size_5-of-12 slds-large-size_5-of-12">
                                    <div class="slds-form-element">
                                        <label class="slds-form-element__label" for="input-id-01">{!v.translatorValue.First_Name_Label__c}</label>
                                        <div class="slds-form-element__control">
                                            <ui:inputText aura:id="itemNameJC" class="slds-input" placeholder="{!v.translatorValue.First_Name_Placeholder__c}" value="{!v.newItem.FirstName}" onError="{!c.handleError}" onClearErrors="{!c.handleClearError}" disabled="{!v.afterPrinting}" />
                                        </div>
                                    </div>
                                </div> 
                                <aura:set attribute="else">
                                    <div class="slds-col slds-size_5-of-12 slds-large-size_5-of-12">
                                        <div class="slds-form-element">
                                            <label class="slds-form-element__label" for="input-id-01">{!v.translatorValue.First_Name_Label__c}</label>  
                                            <div class="slds-form-element__control">
                                                <ui:inputText aura:id="itemName" class="slds-input" placeholder="{!v.translatorValue.First_Name_Placeholder__c}" value="{!v.newItem.FirstName}" onError="{!c.handleError}" onClearErrors="{!c.handleClearError}" disabled="{!v.afterPrinting}" />
                                                
                                            </div>
                                        </div>
                                    </div>
                                    <div class="slds-col slds-size_5-of-12 slds-large-size_5-of-12">
                                        <div class="slds-form-element">
                                            <label class="slds-form-element__label" for="input-id-01">{!v.translatorValue.Last_Name_Label__c}</label>
                                            <div class="slds-form-element__control">
                                                <ui:inputText aura:id="itemSurname" class="slds-input" placeholder="{!v.translatorValue.Last_Name_Placeholder__c}" value="{!v.newItem.LastName}" onError="{!c.handleError}" onClearErrors="{!c.handleClearError}" disabled="{!v.afterPrinting}" />
                                            </div>
                                        </div>
                                    </div>
                                </aura:set>    
                            </aura:if>
                        </div>
                        <aura:if isTrue="{!(v.locale == 'JP'|| v.locale == 'CN' || v.locale == 'RU' || v.locale == 'HK' || v.locale == 'MO' || v.truncLanguage == 'ja'|| v.truncLanguage == 'zhs' || v.truncLanguage == 'zh' || v.truncLanguage == 'ru' )}">
                            <div class="slds-grid slds-wrap row">
                                <div class="slds-col slds-size_6-of-12 slds-large-size_6-of-12">
                                    <div class="slds-form-element">
                                        <label class="slds-form-element__label" for="input-id-01">{!v.translatorValue.Local_Last_Name_Label__c }</label>
                                        <div class="slds-form-element__control">
                                            <ui:inputText aura:id="itemSurnameJP" class="slds-input" placeholder="{!v.translatorValue.Local_Last_Name_Placeholder__c}" value="{!v.newItem.Cognome_UTF_8__c}" onError="{!c.handleError}" onClearErrors="{!c.handleClearError}" disabled="{!v.afterPrinting}" />
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-col slds-size_6-of-12 slds-large-size_6-of-12">
                                    <div class="slds-form-element">
                                        <label class="slds-form-element__label" for="input-id-01">{!v.translatorValue.Local_First_Name_Label__c }</label>
                                        <div class="slds-form-element__control">
                                            <ui:inputText aura:id="itemNameJP" class="slds-input" placeholder="{!v.translatorValue.Local_First_Name_Placeholder__c}" value="{!v.newItem.Nome_UTF_8__c}" onError="{!c.handleError}" onClearErrors="{!c.handleClearError}" disabled="{!v.afterPrinting}" />
                                        </div>
                                    </div>
                                </div>
                            </div> 
                        </aura:if>
                        <!-- US -->

                        <!-- PALUMBO (START) -->

                        <!-- DEFAULT ADDRESS -->
                        <div class="slds-section slds-is-open" aura:id="default-address-id">
                            <h3 class="slds-section__title">
                                <button aria-controls="default-address" aria-expanded="true" data-auraId ="default-address-id" data-iconId ="default-address-icon" class="slds-button slds-section__title-action" onclick="{!c.handleAddressSection}" >
                                    <lightning:icon aura:id="default-address-icon" iconName="utility:switch" size="xx-small" alternativeText="switch" class=""/>
                                    <span data-auraId ="default-address-id" data-iconId ="default-address-icon" class="slds-truncate" title="Section Title"> &nbsp;{!v.translatorValue.Billing_Address_Section_Label__c }</span>
                                </button>
                            </h3> 

                            <div aria-hidden="false" class="slds-section__content" id="default-address">
                                <aura:if isTrue="{!(v.locale == 'JP')}">
                                    <div class="slds-grid slds-wrap row">
                                        <div class="slds-col slds-size_2-of-12 slds-large-size_2-of-12">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label textSizeMobile" for="input-id-01">{!v.translatorValue.CAP_Label__c}</label>
                                                <div class="slds-form-element__control">
                                                    <ui:inputText aura:id="itemPersonMailingPostalCodeJP" class="slds-input" placeholder="{!v.translatorValue.CAP_Placeholder__c}" value="{!v.newItem.PersonMailingPostalCode}" onError="{!c.handleError}" onClearErrors="{!c.handleClearError}" disabled="{!v.afterPrinting}" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="slds-size_2-of-12 slds-large-size_2-of-12">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label" for="input-id-01">{!v.translatorValue.State_Label__c}</label>
                                                <div class="slds-form-element__control">
                                                    <ui:inputText aura:id="itemProvinceJP" class="slds-input" placeholder="{!v.translatorValue.State_Placeholder__c}" value="{!v.newItem.PersonMailingState}" onError="{!c.handleError}" onClearErrors="{!c.handleClearError}" disabled="{!v.afterPrinting}" />
                                                </div>
                                            </div>
                                        </div>
                                        
                                        <div class="slds-size_3-of-12 slds-large-size_3-of-12">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label" for="input-id-01">{!v.translatorValue.City_Label__c}</label>
                                                <div class="slds-form-element__control">
                                                    <ui:inputText aura:id="itemPersonMailingCityJP" class="slds-input" placeholder="{!v.translatorValue.City_Placeholder__c}" value="{!v.newItem.PersonMailingCity}" onError="{!c.handleError}" onClearErrors="{!c.handleClearError}" disabled="{!v.afterPrinting}" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="slds-col slds-size_5-of-12 slds-large-size_5-of-12">
                                            <div class="slds-form-element">
                                                <label class="slds-form-element__label" for="input-id-01">{!v.translatorValue.Street_Label__c}</label>
                                                <div class="slds-form-element__control">
                                                    <ui:inputText aura:id="itemPersonMailingStreetJP" class="slds-input" placeholder="{!v.translatorValue.Street_Placeholder__c}" value="{!v.newItem.PersonMailingStreet}" onError="{!c.handleError}" onClearErrors="{!c.handleClearError}" disabled="{!v.afterPrinting}" />
                                                </div>
                                            </div>
                                        </div>                    
                                    </div>  

                                    <aura:set attribute="else">                                              
                                        <div class="slds-grid slds-wrap row">
                                            <div class="slds-col slds-size_5-of-12 slds-large-size_5-of-12">
                                                <div class="slds-form-element">
                                                    <label class="slds-form-element__label" for="input-id-01">{!v.translatorValue.Street_Label__c}</label>
                                                    <div class="slds-form-element__control">
                                                        <ui:inputText aura:id="itemPersonMailingStreet" class="slds-input" placeholder="{!v.translatorValue.Street_Placeholder__c}" value="{!v.newItem.PersonMailingStreet}" onError="{!c.handleError}" onClearErrors="{!c.handleClearError}" disabled="{!v.afterPrinting}" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="{! 'slds-col' + ' ' +  (v.locale == 'US' ? 'slds-size_2-of-12 slds-large-size_2-of-12' : 'slds-size_4-of-12 slds-large-size_3-of-12' )}">
                                                <div class="slds-form-element">
                                                    <label class="slds-form-element__label" for="input-id-01">{!v.translatorValue.City_Label__c}</label>
                                                    <div class="slds-form-element__control">
                                                        <ui:inputText aura:id="itemPersonMailingCity" class="slds-input" placeholder="{!v.translatorValue.City_Placeholder__c}" value="{!v.newItem.PersonMailingCity}" onError="{!c.handleError}" onClearErrors="{!c.handleClearError}" disabled="{!v.afterPrinting}" />
                                                    </div>
                                                </div>
                                            </div>
                                            <aura:if isTrue="{!v.locale == 'US'}">
                                                <div class="slds-col slds-size_2-of-12 slds-large-size_2-of-12">
                                                    <div class="slds-form-element">
                                                        <label class="slds-form-element__label" for="input-id-01">{!v.translatorValue.State_Label__c}</label>
                                                        <div class="slds-form-element__control">
                                                            <ui:inputText aura:id="itemPersonMailingState" class="slds-input" placeholder="{!v.translatorValue.State_Placeholder__c}" value="{!v.newItem.PersonMailingState}" onError="{!c.handleError}" onClearErrors="{!c.handleClearError}" disabled="{!v.afterPrinting}" />
                                                        </div>
                                                    </div>
                                                </div>
                                            </aura:if>
                                            <div class="slds-col slds-size_3-of-12 slds-large-size_3-of-12">
                                                <div class="slds-form-element">
                                                    <label class="slds-form-element__label textSizeMobile" for="input-id-01">{!v.translatorValue.CAP_Label__c}</label>
                                                    <div class="slds-form-element__control">
                                                        <ui:inputText aura:id="itemPersonMailingPostalCode" class="slds-input" placeholder="{!v.translatorValue.CAP_Placeholder__c}" value="{!v.newItem.PersonMailingPostalCode}" onError="{!c.handleError}" onClearErrors="{!c.handleClearError}" disabled="{!v.afterPrinting}" />
                                                    </div>
                                                </div>
                                            </div>
                                            
                                        </div>    
                                    </aura:set>                            
                                </aura:if>  

                                <div class="slds-col slds-size_4-of-12 slds-large-size_4-of-12">
                                    <!-- <div class="slds-form-element">
                                        <label class="slds-form-element__label" for="input-id-01">{!v.translatorValue.Country_Label__c}</label>
                                        <div class="slds-form-element__control">
                                            <ui:inputSelect class="dynamic slds-select" value="{!v.newItem.PersonMailingCountry}" aura:id="InputSelectDynamicCountry" disabled="{!v.afterPrinting}"/>
                                        </div>
                                    </div> -->
                                    <div class="slds-form-element">
                                        <div class="slds-combobox  slds-combobox-lookup slds-dropdown-trigger slds-dropdown-trigger_click"  aura:id="searchLookupCountry"
                                        aria-expanded="false" aria-haspopup="listbox" role="combobox">
                                        <lightning:input aura:id="inputCountry" name="inputCountry" label="{!v.translatorValue.Country_Label__c}" onfocus="{!c.onFocusPickList}" onchange="{!c.onChangePickList}" onblur="{!c.onBlurPickList}" value="{!v.newItem.PersonMailingCountry}"/>
                                        <div role="listbox">                   
                                            <ul class="slds-listbox slds-listbox_vertical slds-dropdown left1">
                                                <ui:scrollerWrapper class="scrollerSize">
                                                    <aura:iteration items="{!v.customPicklistCountry}" var="option">
                                                        <div>
                                                            <lightning:button name="{!'btn_Country_' + option.value}" class="button-custom" variant="base" label="{!option.label}" value="{!option.value}" onclick="{! c.selectOption }"/>
                                                        </div>
                                                    </aura:iteration>
                                                </ui:scrollerWrapper>
                                                
                                            </ul>
                                        </div>
                                    </div>
                                    </div>
                                </div>
                                
                                <div class="slds-grid slds-wrap row">
                                    <div class="slds-col slds-size_12-of-12 slds-large-size_12-of-12">
                                        <lightning:button variant="brand-outline" aura:id="defaultAddButton" label="{!v.translatorValue.Button_Add_Address_Label__c}" title="{!v.translatorValue.Button_Add_Address_Label__c}" onclick="{! c.handleAddAddress }" iconName="utility:add" disabled="{!v.showContactAddress}"/>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- CONTACT ADDRESS -->
                        <aura:renderif isTrue="{!v.showContactAddress}">
                            <div class="slds-section slds-is-open" aura:id="contact-address-id">
                                <h3 class="slds-section__title">
                                    <button aria-controls="contact-address" aria-expanded="true" data-auraId ="contact-address-id" data-iconId ="contact-address-icon" class="slds-button slds-section__title-action" onclick="{!c.handleAddressSection}" >
                                        <lightning:icon aura:id="contact-address-icon" iconName="utility:switch" size="xx-small" alternativeText="switch" class=""/>
                                        <span data-auraId ="contact-address-id" data-iconId ="contact-address-icon" class="slds-truncate" title="Section Title"> &nbsp;{!v.translatorValue.Contact_Address_Section_Label__c }</span>
                                    </button>
                                </h3> 

                                <div aria-hidden="false" class="slds-section__content" id="contact-address">
                                    <aura:if isTrue="{!(v.locale == 'JP')}">
                                        <div class="slds-grid slds-wrap row">
                                            <div class="slds-col slds-size_2-of-12 slds-large-size_2-of-12">
                                                <div class="slds-form-element">
