<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction,lightning:actionOverride,lightning:isUrlAddressable" access="global" controller="RegistrationForm">
   	<aura:attribute name="textButton" type="String" default="Create new Customer" />
    <aura:attribute name="textVal" type="String" default="" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    
    <div class="slds-text-align_center slds-text-heading_medium margin-text">{!v.textVal}</div>
     <lightning:navigation aura:id="navService"/>
    <ui:button label="{!v.textButton}" press="{!c.openActionWindow}" class="ten_button"/>
</aura:component>