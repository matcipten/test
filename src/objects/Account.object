OR(ISPICKVAL(Categoria_Cliente_Proposta__c, &quot;VIP5&quot;), 
ISPICKVAL(Categoria_Cliente_Proposta__c, &quot;VIP10&quot;), 
ISPICKVAL(Categoria_Cliente_Proposta__c, &quot;VIP15&quot;), 
ISPICKVAL(Categoria_Cliente_Proposta__c, &quot;VIP20&quot;), 
ISPICKVAL(Categoria_Cliente_Proposta__c, &quot;VIP30&quot;), 
ISPICKVAL(Categoria_Cliente_Proposta__c, &quot;VIP40&quot;),
