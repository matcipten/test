        <formula>IF(	AND(Data_Ultimo_Acquisto__c &gt;= (TODAY()-365), Importo_Totale_Acquisti_EUR__c &gt;= 300000 ), &quot;Stars&quot;,
IF( AND(Data_Ultimo_Acquisto__c &gt;= (TODAY()-365), Importo_Totale_Acquisti_EUR__c &gt;= 100000 ) , &quot;Sky&quot;,
IF( AND(Data_Ultimo_Acquisto__c &gt;= (TODAY()-365), Importo_Totale_Acquisti_EUR__c &gt;= 50000 ) , &quot;Mountain&quot;,
IF( AND(Data_Ultimo_Acquisto__c &gt;= (TODAY()-365), Importo_Totale_Acquisti_EUR__c &gt;= 20000 ) , &quot;Trees&quot;,
IF( AND((TODAY()-365) &gt; Data_Ultimo_Acquisto__c,(TODAY()-730) &lt; Data_Ultimo_Acquisto__c, Importo_Totale_Acquisti_EUR__c &gt;= 20000 ) , &quot;Rainbow&quot;,
IF( AND(Data_Ultimo_Acquisto__c &gt;= (TODAY()-365), Importo_Totale_Acquisti_EUR__c &gt;= 5000),&quot;Bush&quot;,
IF( AND(Data_Ultimo_Acquisto__c &gt;= (TODAY()-365), Importo_Totale_Acquisti_EUR__c != 0,Importo_Totale_Acquisti_EUR__c &lt; 5000,N_Scontrini_netto_resi_netto_gift__c = 1) , &quot;Seed&quot;,
IF( AND(Data_Ultimo_Acquisto__c &gt;= (TODAY()-365), Importo_Totale_Acquisti_EUR__c != 0,Importo_Totale_Acquisti_EUR__c &lt; 5000,N_Scontrini_netto_resi_netto_gift__c &gt; 1) , &quot;Grass&quot;,
IF( AND((TODAY()-365) &gt; Data_Ultimo_Acquisto__c,(TODAY()-730) &lt; Data_Ultimo_Acquisto__c, Importo_Totale_Acquisti_EUR__c != 0,Importo_Totale_Acquisti_EUR__c &lt; 20000) , &quot;Rain&quot;,
IF( AND(Data_Ultimo_Acquisto__c &lt;= (TODAY()-730),N_Scontrini_netto_resi_netto_gift__c!=0,Importo_Totale_Acquisti_EUR__c!=0) , &quot;Snow&quot;,
IF( AND(OR(Importo_Totale_Acquisti_EUR__c = 0, Importo_Totale_Acquisti_EUR__c =null)), &quot;Ground&quot;,
null)))))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
