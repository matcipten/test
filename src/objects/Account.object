ISPICKVAL(Categoria_Cliente_Proposta__c, &quot;VJP05&quot;),
ISPICKVAL(Categoria_Cliente_Proposta__c, &quot;VJP10&quot;)),
ISBLANK( Motivazione_Richiesta_Sconto__c ))</errorConditionFormula>
