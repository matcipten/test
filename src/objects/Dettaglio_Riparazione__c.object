    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>Affidamento_e_data_riparazione_prevista</fullName>
        <active>true</active>
        <errorConditionFormula>OR(RecordType.DeveloperName = &quot;Riparazione_Sede&quot;, 
RecordType.DeveloperName = &quot;Riparazione_Customer_Care&quot;) &amp;&amp; 

OR ( 

(NOT(ISBLANK(Affidato_a__c)) &amp;&amp; ISBLANK(Data_riparazione_prevista__c)), 

(ISBLANK(Affidato_a__c)&amp;&amp; NOT(ISBLANK(Data_riparazione_prevista__c))) 
)</errorConditionFormula>
        <errorMessage>Necessario stabilire sia la persona a cui viene affidato il capo, sia prevedere una data di riparazione</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Allegato</fullName>
        <active>false</active>
        <errorConditionFormula>Ho_caricato_l_allegato_richiesto__c = False &amp;&amp; OR (RecordType.DeveloperName = &quot;Riparazione_Sede&quot;, 
RecordType.DeveloperName = &quot;Riparazione_Customer_Care&quot;)</errorConditionFormula>
        <errorMessage>Allegare il file richiesto</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Capo_Consegnato_al_Cliente</fullName>
        <active>true</active>
        <errorConditionFormula>/*OR (
(ISPICKVAL(Status__c, &quot;Chiuso&quot;) &amp;&amp; RecordType.DeveloperName = &quot;Riparazione_Store&quot;),
ISPICKVAL(Status__c, &quot;Riconsegna al cliente effettuata&quot;))*/

ISPICKVAL(Status__c, &quot;Chiuso&quot;)
&amp;&amp; ISBLANK(Note_Avvenuta_Riconsegna__c)</errorConditionFormula>
        <errorDisplayField>Note_Avvenuta_Riconsegna__c</errorDisplayField>
        <errorMessage>A operazione conclusa è necessario aggiungere una nota riguardante l&apos;avvenuta riconsegna</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invio_capo_verso_sede_CC</fullName>
        <active>true</active>
        <errorConditionFormula>AND (RecordType.DeveloperName = &quot;Riparazione_Customer_Care&quot;,
ISPICKVAL(Status__c, &quot;Ricevimento capo danneggiato in sede&quot;), OR (ISBLANK( Tracking_Number_ritiro__c ),
    ISBLANK(Data_Pickup__c),
    ISBLANK(Pickup_Number__c),
    ISBLANK(TEXT(Corriere_rientro__c))))</errorConditionFormula>
        <errorMessage>Prima di inviare il capo è necessario inserire il tracking number, la pickup date, il pickup number e il corriere di rientro</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invio_capo_verso_sede_sede</fullName>
        <active>true</active>
        <errorConditionFormula>AND(RecordType.DeveloperName = &quot;Riparazione_Sede&quot;, 
ISPICKVAL(Status__c, &quot;Ricevimento capo danneggiato in sede&quot;), ISBLANK( Tracking_Number_ritiro__c ))</errorConditionFormula>
        <errorDisplayField>Tracking_Number_ritiro__c</errorDisplayField>
        <errorMessage>Prima di inviare il capo è necessario inserire il tracking number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Limite_Status_Boutique</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED (Status__c) &amp;&amp; 
OR(ISPICKVAL(Status__c, &quot;Invio capo verso sede&quot;), 
ISPICKVAL(Status__c, &quot;Riparazione effettuata&quot;), 
ISPICKVAL(Status__c, &quot;Spedizione capo riparato effettuata&quot;), 
ISPICKVAL(Status__c, &quot;Ricevimento capo riparato in boutique&quot;)) 
&amp;&amp; OR($Profile.Id = $Setup.Profile__c.Id_Boutique__c,
$Profile.Id = $Setup.Profile__c.Id_Associate__c)</errorConditionFormula>
        <errorMessage>Lo step selezionato non è in carico al negozio</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Note_Avvenuta_Riconsegna</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK(Note_Avvenuta_Riconsegna__c)) &amp;&amp;  TEXT(Status__c) &lt;&gt; &quot;Riconsegna al cliente effettuata&quot; &amp;&amp;  TEXT(Status__c) &lt;&gt; &quot;Aperto&quot; &amp;&amp;  TEXT(Status__c) &lt;&gt; &quot;Chiuso&quot;  &amp;&amp;  TEXT(Status__c) &lt;&gt; &quot;Ricevimento capo riparato in boutique&quot;</errorConditionFormula>
        <errorDisplayField>Note_Avvenuta_Riconsegna__c</errorDisplayField>
        <errorMessage>Possono essere inserite note di avvenuta riconsegna solo al momento dell&apos;effettiva riconsegna al cliente del capo riparato</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Numero_di_autorizzazione_inserito</fullName>
        <active>true</active>
        <errorConditionFormula>(ISPICKVAL(Status__c, &quot;Invio capo verso sede&quot;) &amp;&amp; ISBLANK(Numero_Autorizzazione_Sede__c)) 

||

(ISPICKVAL(Riparazione_autorizzata__c, &quot;Sì&quot;) &amp;&amp; ISBLANK(Numero_Autorizzazione_Sede__c))</errorConditionFormula>
        <errorDisplayField>Numero_Autorizzazione_Sede__c</errorDisplayField>
        <errorMessage>A richiesta approvata, è necessario inserire il numero di autorizzazione</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prodotto_Riparato_e_Note</fullName>
        <active>true</active>
        <errorConditionFormula>OR(RecordType.DeveloperName = &quot;Riparazione_Sede&quot;, 
RecordType.DeveloperName = &quot;Riparazione_Customer_Care&quot;) &amp;&amp; 

OR ( 

(NOT(ISBLANK(text(Prodotto_riparato__c))) &amp;&amp; ISBLANK(Riparazione_completata_Note__c)), 

(ISBLANK(text(Prodotto_riparato__c))&amp;&amp; NOT(ISBLANK(Riparazione_completata_Note__c))) 
)</errorConditionFormula>
        <errorMessage>Necessario specificare sia il livello di riparazione del prodotto, sia una nota riguardante l&apos;avvenuta riparazione</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ricevimento_capo_danneggiato_in_sede</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Status__c, &quot;Riparazione effettuata&quot;) &amp;&amp; OR( ISBLANK(Affidato_a__c), ISBLANK(Data_riparazione_prevista__c) )</errorConditionFormula>
        <errorMessage>Quando il capo arriva in sede, è necessario stabilire la persona a cui viene affidato e prevedere una data di riparazione</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Riparazione_effettuata</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Status__c, &quot;Spedizione capo riparato effettuata&quot;) &amp;&amp; ISBLANK(Riparazione_completata_Note__c)</errorConditionFormula>
        <errorDisplayField>Riparazione_completata_Note__c</errorDisplayField>
        <errorMessage>A riparazione effettuata è necessario aggiungere una nota riguardante l&apos;avvenuta riparazione</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
