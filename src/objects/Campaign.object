    <validationRules>
        <fullName>Invitation_channel_compilato</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
OR(RecordType.DeveloperName = &apos;Events&apos;,RecordType.DeveloperName = &apos;One_To_One&apos;, RecordType.DeveloperName = &apos;Mailing&apos;),
Cards__c=false, Email__c =false, Chat__c =false)</errorConditionFormula>
        <errorMessage>Please select at least one invitation channel.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Location_alternative</fullName>
        <active>true</active>
        <errorConditionFormula>AND(!ISBLANK(Location_Instore__c), !ISBLANK( Location_Out_of_store__c ))</errorConditionFormula>
        <errorMessage>Please select only one location.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Location_blank</fullName>
        <active>true</active>
        <errorConditionFormula>oR( 
AND(ISPICKVAL( Location__c, &apos;Instore&apos;), ISBLANK(Location_Instore__c)), 

AND(ISPICKVAL( Location__c, &apos;Out of store&apos;), 
ISBLANK( Location_Out_of_store__c)))</errorConditionFormula>
        <errorMessage>Please select &quot;Where&quot; if Location is &quot;Out of store&quot;, select &quot;Boutique&quot; if Location is &quot;Instore&quot;.</errorMessage>
    </validationRules>
