    <rules>
        <fullName>Ecommerce Case %22Notify Me - Non trovi la tua taglia%3F%22 ROW</fullName>
        <actions>
            <name>Auto_Response_Ecommerce_No_Taglia_NO_USA</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <formula>AND (RecordType.DeveloperName = &quot;Ticket_E_commerce&quot;, 
Nazione_input__c &lt;&gt; &quot;US&quot;, 
BEGINS(Subject, &quot;I can&apos;t find my size online&quot;))</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Ecommerce Case %22Notify Me - Non trovi la tua taglia%3F%22 ROW - Man</fullName>
        <actions>
            <name>Auto_Response_Ecommerce_Notify_Me_Non_trovi_la_tua_taglia_ROW_Man</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <description>Risposta automatica al Caso Ticket Ecommerce &quot;Notify Me - Non trovi la tua taglia?&quot; - Italy e ROW - Man</description>
        <formula>AND (RecordType.DeveloperName = &quot;Ticket_E_commerce&quot;, 
     Nazione_input__c &lt;&gt; &quot;US&quot;, 
     BEGINS(Subject, &quot;I can&apos;t find my size online&quot;), 
     OR (Titolo_Cliente_input__c = &quot;Mr&quot;, 
         Titolo_Cliente_input__c = &quot;Herr&quot;, 
         Titolo_Cliente_input__c = &quot;Sig.&quot;, 
         Titolo_Cliente_input__c = &quot;Г-н&quot;, 
         Titolo_Cliente_input__c = &quot;男性&quot;, 
         Titolo_Cliente_input__c = &quot;先生&quot;))</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
