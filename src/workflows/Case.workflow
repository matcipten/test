    <rules>
        <fullName>Auto-Response Ticket Ecomm ROW</fullName>
        <actions>
            <name>Auto_response_Ticket_Ecommerce_ROW</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Case.RecordTypeId</field>
            <operation>equals</operation>
            <value>E-commerce Ticket</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Nazione_input__c</field>
            <operation>notEqual</operation>
            <value>US</value>
        </criteriaItems>
        <description>Auto-Response Ticket Ecommerce ROW</description>
        <triggerType>onCreateOnly</triggerType>
    </rules>
